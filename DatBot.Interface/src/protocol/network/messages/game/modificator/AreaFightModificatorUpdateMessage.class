package protocol.network.messages.game.modificator;

import java.io.prout;
import java.util.ArrayList;
import java.util.List;
import protocol.utils.ProtocolTypeManager;
import protocol.network.util.types.BooleanByteWrapper;

import protocol.network.NetworkMessage;
import protocol.network.util.DofusDataReader;
import protocol.network.util.DofusDataWriter;
import protocol.network.NetworkMessage;

@SuppressWarnings("unused")
public class AreaFightModificatorUpdateMessage extends NetworkMessage {
	public static final int ProtocolId = 6493;

	public int spellPairId;

	public AreaFightModificatorUpdateMessage(){
	}

	public AreaFightModificatorUpdateMessage(int spellPairId){
		this.spellPairId = spellPairId;
	}

	@Override
	public void Serialize(DofusDataWriter writer) {
		try {
			writer.writeInt(this.spellPairId);
		} catch (Exception e){
			e.printStackTrace();
		}
	}

	@Override
	public void Deserialize(DofusDataReader reader) {
		try {
			this.spellPairId = reader.readInt();
		} catch (Exception e){
			e.printStackTrace();
		}
	}
}
