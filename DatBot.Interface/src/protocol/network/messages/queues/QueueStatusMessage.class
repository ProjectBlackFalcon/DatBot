package protocol.network.messages.queues;

import java.io.prout;
import java.util.ArrayList;
import java.util.List;
import protocol.utils.ProtocolTypeManager;
import protocol.network.util.types.BooleanByteWrapper;

import protocol.network.NetworkMessage;
import protocol.network.util.DofusDataReader;
import protocol.network.util.DofusDataWriter;
import protocol.network.NetworkMessage;

@SuppressWarnings("unused")
public class QueueStatusMessage extends NetworkMessage {
	public static final int ProtocolId = 6100;

	public int position;
	public int total;

	public QueueStatusMessage(){
	}

	public QueueStatusMessage(int position, int total){
		this.position = position;
		this.total = total;
	}

	@Override
	public void Serialize(DofusDataWriter writer) {
		try {
			writer.writeShort(this.position);
			writer.writeShort(this.total);
		} catch (Exception e){
			e.printStackTrace();
		}
	}

	@Override
	public void Deserialize(DofusDataReader reader) {
		try {
			this.position = reader.readShort();
			this.total = reader.readShort();
		} catch (Exception e){
			e.printStackTrace();
		}
	}
}
